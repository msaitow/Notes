  issn={0021-9606},
  file={/Users/saitow/Documents/ReadCube Media/Yang et al-2012-The Journal of chemical physics.pdf;/Users/saitow/Documents/ReadCube Media/Yang et al-2012-The Journal of chemical physics 1.pdf;/Users/saitow/Documents/ReadCube Media/Yang et al-2012-The Journal of chemical physics 2.pdf;/Users/saitow/Documents/papers/http---scitation.aip.org-getpdf-servlet-GetPDFServlet?filetype=pdf&id=JCPSA6000136000014144105000001&idtype=cvips&doi=10.1063-1.pdf;/Users/saitow/Documents/ReadCube Media/J Chem Phys 2012 Yang J.pdf},
